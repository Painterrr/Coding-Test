def solution(skill, skill_trees):
    answer = 0
    
    for now in skill_trees:
        idx = 0
        flag = True
        
        for s in now:
            if idx == len(skill) - 1:
                break
            if s == skill[idx]:
                idx += 1
                continue
            if s in skill[idx + 1:]:
                flag = False
                break
        if flag:
            answer += 1
    
    return answer
